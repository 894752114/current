{
  "manifest_version": 2,
  "name": "入门示例",
  "version": "1.0",
  "description": "构建扩展!",
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "browser_action": 
	{
		"default_icon": "images/icon.png",
		"default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.4.1.js"]
    }
  ]
}

